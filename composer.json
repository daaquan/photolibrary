{
    "name": "robbertkl/photolibrary",
    "description": "Simple library for reading iPhoto '11 .photolibrary packages",
    "keywords": ["apple", "iphoto"],
    "homepage": "https://github.com/robbertkl/photolibrary",
    "license": "MIT",
    "authors": [
        {
            "name": "Robbert Klarenbeek",
            "email": "robbertkl@renbeek.nl"
        }
    ],
    "support": {
        "issues": "https://github.com/robbertkl/photolibrary/issues",
        "source": "https://github.com/robbertkl/photolibrary"
    },
    "autoload": {
        "psr-0": {
            "PhotoLibrary\\": "classes/"
        }
    },
    "require": {
        "php": ">=5.3",
        "rodneyrehm/plist": "~2.0"
    },
    "require-dev": {
        "zendframework/zend-cache": "~2.0",
        "zendframework/zend-serializer": "~2.0",
        "apigen/apigen": "2.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "suggest": {
        "zendframework/zend-cache": "~2.0, required to enable caching, which is recommended for large photo libraries",
        "zendframework/zend-serializer": "~2.0, required when using a cache storage adapter that doesn't support array datatypes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
